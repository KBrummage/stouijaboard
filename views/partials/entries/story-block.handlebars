<div class="card" id="{{id}}">
      <div class="card-header">
            Created by: {{#each users}}{{email}}{{/each}}, Created on: {{createdAt}}, Last contributed: {{updatedAt}}
      </div>
      <div class="card-body">
            <h4 class="card-title">{{title}}</h4>
            <p class="card-text">{{entry}}</p>
            <a type="submit" id="{{id}}" class="contribute btn btn-primary">Contribute to This Story</a>
            
      </div>      
</div>
      

