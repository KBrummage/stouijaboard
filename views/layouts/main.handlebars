<body class="mainContainer">
  <!--Nav Bar-->
  <div id="container">
    <nav class="navbar navbar-expand-lg navbar-light bg-light fixed">
      <a class="navbar-brand" href="#">Stouiji Board</a>
      <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent"
        aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>

      <div class="collapse navbar-collapse" id="navbarSupportedContent">
        <ul class="navbar-nav mr-auto">
          <li class="nav-item active">
            <a class="nav-link" href="#">Home
              <span class="sr-only">(current)</span>
            </a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="#">New Story
            </a>
          </li>
          <li class="nav-item dropdown">
            <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true"
              aria-expanded="false">
              Sort By...
            </a>
            <div class="dropdown-menu" aria-labelledby="navbarDropdown">
              <a class="dropdown-item" href="#">Recent</a>
              <a class="dropdown-item" href="#">Most Active</a>
              <div class="dropdown-divider"></div>
              <a class="dropdown-item" href="#">Something else here</a>
            </div>
          </li>
        </ul>

        <div class="form horiz">
          <button class="btn btn-outline-success my-2 my-sm-0" type="submit" class="btn btn-default" id="loginBtn">Login</button>
          <button class="btn btn-outline-success my-2 my-sm-0" type="submit" class="btn btn-default" id="signUpBtn">Sign Up!
          </button>

        </div>

      </div>

    </nav>

  </div>
  <!--Ouiji Board Graphic-->
  <div class="container ouiji">
    <div class="module parallax" id="module"></div>
